{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    'my-custom-rules/variable-name-length': 'Variable names must be no shorter than 5 characters and no longer than 20 characters',
    'my-custom-rules/variable-name-camelcase': 'Variable names must be written in camel case'
    }
}
