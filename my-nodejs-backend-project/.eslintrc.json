{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["custom"],
    "rules": {
        "custom/variable-name-camelcase": "error",
        "custom/variable-name-length": "error"
    }
}